{{>layouts/header}}
    <table border="1">
        <thead>
            <tr>
                <td>ID</td>
                <td>Title</td>
                <td>Content</td>
            </tr>
        </thead>
        <tbody>
            {{#articleList}}<!-- 이거 안쓰고 article.id 이렇게도 가능함-->
                <tr>
                    <td>{{id}}</td>
                    <td><a href="/articles/{{id}}"> {{title}} </a></td>
                    <td>{{content}}</td>
                </tr>
            {{/articleList}}
        </tbody>
    </table>
    <a href="/articles/new">글쓰기</a>
{{>layouts/footer}}